<% include partials/header%>



<div class="container">
<header class="jumbotron">
    <div class="container">
        <h1>Welcome to Rent-A-Palace</h1>
        <p>Here are our hanpicked palaces for you to rent</p>
        <p>
            <a class="btn btn-primary brn-lg" href="/palaces/new">Add New Palaces</a>

        </p>
    </div>
</header>

<div class="row text-center" style="display:flex; flex-wrap: wrap;">
    <% palaces.forEach(function (palaces) { %>
        <div class="col-md-3 col-sm-6">
            <div class="thumbnail">
                <img src="<%= palaces.image %>" alt="">
                <div class="caption">
                    <h4><%= palaces.name%></h4>
                </div>
                <p>
                    <a href="palaces/<%= palaces._id %>" class="btn btn-primary">More Info</a>
                </p>
            </div>

        </div>
    <%}); %>
</div>


</div>
<%include partials/footer%>

